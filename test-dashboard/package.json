{
  "name": "visual-memory-test-dashboard",
  "version": "1.0.0",
  "description": "Test Dashboard for Visual Memory Search Yantra",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test:backend": "cd ../backend && python -m pytest test_main.py --json-report --json-report-file=../test-dashboard/test-results/backend-results.json",
    "test:frontend": "cd ../frontend && npm test -- --watchAll=false --coverage --coverageReporters=json --coverageDirectory=../test-dashboard/test-results",
    "test:all": "npm run test:backend && npm run test:frontend",
    "start": "npm run dev"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "lucide-react": "^0.294.0",
    "recharts": "^2.8.0",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}